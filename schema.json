        "tags": ["auth"]
      }
    },
    "/api/auth/login/": {
      "post": {
        "operationId": "auth_login",
        "parameters": [],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/AuthLoginSchema"
              }
            }
          },
          "required": true
        },
        "responses": {
          "204": {
            "description": "No Content"
          },
          "400": {
            "description": "username or password not provided"
          },
          "401": {
            "description": "invalid username or password provided"
          }
        },
        "summary": "Login",
        "tags": ["auth"]
      }
    },
    "/api/auth/logout/": {
      "post": {
